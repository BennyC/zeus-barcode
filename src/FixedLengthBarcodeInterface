<?php

namespace Zeus\Barcode;

/**
 * Identifies a barcode that have a fixed length of data.
 * 
 * @author Rafael M. Salvioni
 */
interface FixedLengthBarcodeInterface extends BarcodeInterface
{
    /**
     * Returns the barcode data length.
     * 
     * @return int
     */
    public function getLength();
}
